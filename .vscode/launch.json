{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: debug r3snyk-tool",
            "type": "debugpy",
            "request": "launch",
            "module": "r3snyk.r3snyk",
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "jmad",
                "-n",
                "cenm-relpack",
                "-v",
                "-i",
                "ENT-10412",
                "-c",
                "CENM 1.5 is now out of support."
            ]
        }
    ]
}
